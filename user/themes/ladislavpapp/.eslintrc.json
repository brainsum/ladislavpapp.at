{
  "extends": "airbnb-base",
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "_": true
  },
  "rules": {
    "consistent-return": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "max-nested-callbacks": [
      1,
      3
    ],
    "no-plusplus": [
      1,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-param-reassign": [
      0
    ],
    "no-prototype-builtins": [
      0
    ],
    "no-multi-spaces": [
      "error",
      {
        "exceptions": {
          "ImportDeclaration": true,
          "VariableDeclarator": true,
          "Property": true
        }
      }
    ],
    "valid-jsdoc": [
      1,
      {
        "prefer": {
          "returns": "return",
          "property": "prop"
        },
        "requireReturn": false
      }
    ],
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "no-unused-vars": [
      1
    ]
  }
}
